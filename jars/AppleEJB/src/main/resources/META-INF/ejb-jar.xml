<?xml version="1.0" encoding="UTF-8"?>
<ejb-jar xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/ejb-jar_3_1.xsd" version="3.1">
  <display-name>AppleEJB</display-name>
  <enterprise-beans>
    <session>
      <ejb-name>AppleBean</ejb-name>
      <ejb-ref>
        <ejb-ref-name>ejb/Orange</ejb-ref-name>
        <ejb-ref-type>Session</ejb-ref-type>
        <remote>com.github.tkmtmkt.Orange</remote>
      </ejb-ref>
    </session>
  </enterprise-beans>
</ejb-jar>
